					mapOfURLParameters.put('questionId', notificationObjArg.Object_Id__c);
				}
				
			} else if('Answer'.equalsIgnorecase(notificationObjArg.Object_Type__c)) {
				mapOfURLParameters.put('siteId', notificationObjArg.Simpplr_Site__c);
